{
  "env": {
    "execution.parallelism": 1,
    "execution.time-characteristic": "event-time",
    "job.name": "wattttt"
  },
  "source": [
    {
      "plugin_name": "DataGenTableStream",
      "result_table_name": "dataGenTableStreamTable",
      "schema": "{\"id\":1, \"host\":\"192.168.1.103\",\"source\":\"datasource\",\"MetricsName\":\"cpu\",\"value\":49}",
      "rows-per-second": 10000,
      "number-of-rows": 100000000,
      "fields": [
        {
          "id": {
            "type": "Int",
            "min": 1,
            "max": 2
          }
        },
        {
          "host": {
            "type": "String",
            "length": 5
          }
        },
        {
          "source": {
            "type": "String",
            "length": 10
          }
        },
        {
          "MetricsName": {
            "type": "String",
            "length": 10
          }
        },
        {
          "value": {
            "type": "Int",
            "min": 1,
            "max": 2
          }
        }
      ],
      "parallelism": 10,
      "name": "my-datagen-source"
    }
  ],
  "transform": [
    {
      "source_table_name": "dataGenTableStreamTable",
      "result_table_name": "FieldOperation_time",
      "plugin_name": "FieldOperation",
      "target_field": "_time",
      "script": "UNIX_TIMESTAMP()*1000"
    }
  ],
  "sink": [
    {
      "source_table_name": "FieldOperation_time",
      "plugin_name": "KafkaTable",
      "producer.bootstrap.servers": "192.168.1.218:9092",
      "topics": "test",
      "parallelism": 10,
      "name": "my-kafka-sink"
    }
  ]
}